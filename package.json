{
  "name": "x-vui",
  "version": "1.3.0",
  "description": "A Private Component Library for Vue.js .",
  "main": "lib/vui.js",
  "style": "lib/vui-css/index.css",
  "files": [
    "lib",
    "src",
    "packages"
  ],
  "private": false,
  "scripts": {
    "bootstrap": "cnpm install",
    "dev": "npm run bootstrap && webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "deploy": "npm run deploy:docs && gh-pages -d examples/dist",
    "deploy:docs": "rimraf examples/dist && node build/build.js",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "dist": "npm run clean && npm run lint && npm run build:vui && npm run build:vui-css",
    "release": "npm run bootstrap && sh build/release.sh",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "test:coverage": "find test/unit/coverage/lcov-report -name 'index.html' | sed -n 1,1p  | xargs -I {} open {}  ",
    "lint": "eslint --ext .js,.vue src test/unit",
    "build:vui": "webpack --progress --hide-modules --config build/webpack.build.js && rimraf examples/dist && cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.build.min.js",
    "build:vui-css": "gulp build --gulpfile packages/vui-css/gulpfile.js && rimraf lib/vui-css && cp-cli packages/vui-css/lib lib/vui-css"
  },
  "keywords": [
    "vui",
    "vue",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brickies/vui.git"
  },
  "author": "qiangdada",
  "license": "MIT",
  "homepage": "https://github.com/Brickies/vui.git",
  "bugs": {
    "url": "https://github.com/Brickies/vui/issues"
  },
  "peerDependencies": {
    "vue": "^2.5.2"
  },
  "dependencies": {
    "vue": "^2.5.2",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "avoriaz": "^6.3.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^4.1.2",
    "chalk": "^2.0.1",
    "child_process": "^1.0.2",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.0.1",
    "cp-cli": "^1.1.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "fast-vue-md-loader": "^1.0.3",
    "file-loader": "^1.1.4",
    "file-save": "^0.2.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gh-pages": "^1.1.0",
    "highlight.js": "^9.12.0",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "inquirer": "^5.1.0",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "node-notifier": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "phantomjs-prebuilt": "^2.1.14",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-mixins": "^6.2.0",
    "postcss-salad": "^2.0.1",
    "postcss-url": "^7.2.1",
    "precss": "^3.1.2",
    "rimraf": "^2.6.0",
    "saladcss-bem": "^0.0.2",
    "semver": "^5.3.0",
    "shelljs": "^0.8.5",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.8.0",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.3.0",
    "vue-markdown-loader": "^2.4.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
